{
  "plugins": [
    "lodash"
  ],
  "presets": [
    "env",
    "react"
  ],
  "ignore": [
    "assets/**",
    "dist/**",
    "output/**",
    "reports/**",
    "tmp/**"
  ],
  "env": {
    "cover": {
      "plugins": [
        [
          "istanbul", {
            "exclude": [
              "!**/node_modules/**",
              "**/tests/**"
            ]
          }
        ]
      ]
    }
  }
}
